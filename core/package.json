    "typedoc": "^0.27.7",
    "typescript": "^5.7.3",
    "vitest": "^3.0.5"
  },
  "peerDependencies": {
    "@lightprotocol/hasher.rs": ">=0.2.0",
    "@solana/web3.js": ">=1.90.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
