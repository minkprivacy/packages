{
  "name": "@minkprivacy/core",
  "version": "0.1.1",
  "description": "Mink Privacy SDK - Core library for private Solana transactions",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./crypto": {
      "import": {
        "types": "./dist/crypto/index.d.ts",
        "default": "./dist/crypto/index.js"
