      }
    },
    "./inbox": {
      "import": {
        "types": "./dist/inbox/index.d.ts",
        "default": "./dist/inbox/index.js"
      },
      "require": {
        "types": "./dist/inbox/index.d.cts",
        "default": "./dist/inbox/index.cjs"
      }
    },
    "./viewing-keys": {
      "import": {
        "types": "./dist/viewing-keys/index.d.ts",
        "default": "./dist/viewing-keys/index.js"
      },
      "require": {
        "types": "./dist/viewing-keys/index.d.cts",
        "default": "./dist/viewing-keys/index.cjs"
      }
