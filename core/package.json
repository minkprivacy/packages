  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "docs": "typedoc"
  },
  "keywords": [
    "solana",
    "privacy",
    "zk",
    "stealth",
    "mink",
    "sdk",
    "core"
  ],
  "author": "Mink Privacy",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/minkprivacy/packages",
    "directory": "packages/core"
  },
  "homepage": "https://github.com/minkprivacy/packages#readme",
  "bugs": {
    "url": "https://github.com/minkprivacy/packages/issues"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.31.1",
    "@noble/curves": "^1.4.0",
