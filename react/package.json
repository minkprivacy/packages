{
  "name": "@minkprivacy/react",
  "version": "0.1.0",
  "description": "Mink Privacy React SDK - React bindings for Mink Privacy SDK",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "docs": "typedoc"
  },
  "keywords": [
    "solana",
    "privacy",
    "zk",
    "mink",
    "react",
    "hooks"
  ],
  "author": "Mink Privacy",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/minkprivacy/packages",
    "directory": "packages/react"
  },
  "homepage": "https://github.com/minkprivacy/packages#readme",
  "bugs": {
    "url": "https://github.com/minkprivacy/packages/issues"
  },
  "dependencies": {},
  "devDependencies": {
    "@minkprivacy/core": "workspace:*",
    "@solana/wallet-adapter-react": "^0.15.0",
    "@solana/web3.js": "^1.98.4",
    "@testing-library/react": "^16.2.0",
    "@types/react": "^18.3.18",
    "react": "^18.3.1",
    "tsup": "^8.3.6",
    "typedoc": "^0.27.7",
    "typescript": "^5.7.3",
    "vitest": "^3.0.5"
  },
  "peerDependencies": {
    "@minkprivacy/core": ">=0.1.0",
    "@solana/wallet-adapter-react": ">=0.15.0",
    "react": ">=18.0.0"
  },
  "engines": { "node": ">=18.0.0" }
}
