        "types": "./dist/proofs/index.d.ts",
        "default": "./dist/proofs/index.js"
      },
      "require": {
        "types": "./dist/proofs/index.d.cts",
        "default": "./dist/proofs/index.cjs"
      }
    },
    "./merkle": {
      "import": {
        "types": "./dist/merkle/index.d.ts",
