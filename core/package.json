      },
      "require": {
        "types": "./dist/errors/index.d.cts",
        "default": "./dist/errors/index.cjs"
      }
    },
    "./network": {
      "import": {
        "types": "./dist/network/index.d.ts",
        "default": "./dist/network/index.js"
      },
      "require": {
        "types": "./dist/network/index.d.cts",
        "default": "./dist/network/index.cjs"
      }
    },
    "./models": {
      "import": {
        "types": "./dist/models/index.d.ts",
